{
  "name": "js-algo",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/index.ts",
    "build": "bun build src/index.ts",
    "test": "bun test src/index.ts",
    "watch": "bun --hot src/index.ts",
    "start:backend": "bun run src/backend/index.ts",
    "dev:backend": "bun run --hot src/backend/index.ts 8081",
    "build:frontend": "bun build --minify src/frontend/index.ts --outdir www/js",
    "dev:frontend": "bun --minify --watch build src/frontend/index.ts --outdir www/js"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}